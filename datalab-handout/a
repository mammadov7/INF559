create table person(pid int primary key, firstname varchar(128), lastname varchar(128));
create table office(oid int primary key, positiontitle varchar(128), details varchar(128));
create table heldOffice(pid int, oid int, ystart int, yend int,
constraint fk_pid foreign key(pid) references person(pid),
constraint fk_oid foreign key(oid) references office(oid)
);



